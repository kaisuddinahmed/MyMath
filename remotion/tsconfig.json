{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "render-api.ts"],
  "exclude": ["node_modules", "dist"]
}
